generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model bairrosinteressetocomprador {
  A Int
  B Int

  @@unique([A, B], map: "_BairrosInteresseToComprador_AB_unique")
  @@index([B], map: "_BairrosInteresseToComprador_B_index")
  @@map("_bairrosinteressetocomprador")
}

model aluguelapartamento {
  id              Int    @id @default(autoincrement())
  interesse       String @default("ALUGUEL")
  area_construida Float
  quartos         Int
  banheiros       Int
  vagas_garagem   Int
  andar           Int
  condominio      String
  valor_cond      Float
}

model aluguelcasa {
  id              Int    @id @default(autoincrement())
  interesse       String @default("ALUGUEL")
  area_construida Float
  quartos         Int
  banheiros       Int
  vagas_garagem   Int
  posicao_imovel  String
  condominio      String
  valor_cond      Float
}

model bairrosinteresse {
  id   Int    @id @default(autoincrement())
  nome String
}

model caracteristicasaluguel {
  id                 Int     @id @default(autoincrement())
  aluguel_casa_id    Int
  ar_condicionado    Boolean
  area_servico       Boolean
  armarios_cozinha   Boolean
  armarios_quarto    Boolean
  mobiliado          Boolean
  varanda            Boolean
  area_murada        Boolean
  condominio_fechado Boolean
  permitido_animais  Boolean
  piscina            Boolean
  portao_eletronico  Boolean

  @@index([aluguel_casa_id], map: "CaracteristicasAluguel_aluguel_casa_id_fkey")
}

model caracteristicasaluguelapartamento {
  id                     Int     @id @default(autoincrement())
  aluguel_apartamento_id Int
  ar_condicionado        Boolean
  area_servico           Boolean
  armarios_cozinha       Boolean
  armarios_quarto        Boolean
  mobiliado              Boolean
  varanda                Boolean
  area_murada            Boolean
  condominio_fechado     Boolean
  permitido_animais      Boolean
  piscina                Boolean
  portao_eletronico      Boolean

  @@index([aluguel_apartamento_id], map: "CaracteristicasAluguelApartamento_aluguel_apartamento_id_fkey")
}

model caracteristicascompraapartamento {
  id                    Int     @id @default(autoincrement())
  compra_apartamento_id Int
  ar_condicionado       Boolean
  area_servico          Boolean
  armarios_cozinha      Boolean
  armarios_quarto       Boolean
  mobiliado             Boolean
  varanda               Boolean
  area_murada           Boolean
  condominio_fechado    Boolean
  piscina               Boolean
  portao_eletronico     Boolean

  @@index([compra_apartamento_id], map: "CaracteristicasCompraApartamento_compra_apartamento_id_fkey")
}

model caracteristicascondominio {
  id                 Int     @id @default(autoincrement())
  imovel_id          Int
  tipo_imovel        String
  area_murada        Boolean
  condominio_fechado Boolean
  permitido_animais  Boolean
  piscina            Boolean
  portao_eletronico  Boolean
  seguranca_24h      Boolean

  @@index([imovel_id], map: "CaracteristicasCondominio_imovel_id_fkey")
}

model caracteristicasvenda {
  id                 Int     @id @default(autoincrement())
  venda_casa_id      Int
  ar_condicionado    Boolean
  area_servico       Boolean
  armarios_cozinha   Boolean
  armarios_quarto    Boolean
  mobiliado          Boolean
  varanda            Boolean
  area_murada        Boolean
  condominio_fechado Boolean
  piscina            Boolean
  portao_eletronico  Boolean

  @@index([venda_casa_id], map: "CaracteristicasVenda_venda_casa_id_fkey")
}

model compraapartamento {
  id              Int    @id @default(autoincrement())
  interesse       String @default("VENDA")
  area_construida Float
  quartos         Int
  banheiros       Int
  vagas_garagem   Int
  andar           Int
  condominio      String
  valor_cond      Float
}

model comprador {
  id               Int    @id @default(autoincrement())
  nome             String
  telefone         String
  valor_disponivel Float
}

model interesseimovel {
  id             Int      @id @default(autoincrement())
  comprador_id   Int
  imovel_id      Int
  tipo_imovel    String
  data_interesse DateTime @default(now())

  @@index([comprador_id], map: "InteresseImovel_comprador_fkey")
  @@index([imovel_id], map: "InteresseImovel_venda_casa_fkey")
}

model vendacasa {
  id              Int    @id @default(autoincrement())
  interesse       String @default("VENDA")
  area_construida Float
  quartos         Int
  banheiros       Int
  vagas_garagem   Int
  posicao_imovel  String
  condominio      String
  valor_cond      Float
}

model visitas {
  id           Int      @id @default(autoincrement())
  comprador_id Int
  imovel_id    Int
  data_visita  DateTime @default(now())
  comentario   String?

  @@index([comprador_id], map: "Visitas_comprador_fkey")
  @@index([imovel_id], map: "Visitas_imovel_fkey")
}